/* pages/model/model.wxss */
.wrapper{
    position: fixed;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.model-name{
    text-align: center;
    background: white;
    height: 50px;
    line-height: 50px;
    font-size: 25px;
    margin: 5px 0;
}

swiper{
    height: 610rpx;
}

swiper-item{
    display: flex;
    justify-content: center;
}

image{
    width: 750rpx;
    height: 563rpx;
}

button{
    width: 100%;
    background: lightskyblue;
    height: 40px;
    line-height: 40px;
    font-size: 20px;
    border-radius: 0;
}